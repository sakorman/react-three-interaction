{
	"name": "react-three-interaction",
	"version": "1.0.1",
	"description": "A React-based Three.js interaction library with built-in tools for selecting, hovering, moving, and adding 3D models",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "webpack serve --mode development",
		"build": "webpack --mode production",
		"build:types": "tsc --emitDeclarationOnly",
		"lint": "eslint src --fix",
		"lint:check": "eslint src",
		"type-check": "tsc --noEmit",
		"test": "jest",
		"prepare": "husky install",
		"example": "cd examples && npm start"
	},
	"keywords": [
		"react",
		"threejs",
		"3d",
		"interaction",
		"typescript",
		"editor",
		"visualization"
	],
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sakorman/react-three-interaction.git"
	},
	"bugs": {
		"url": "https://github.com/sakorman/react-three-interaction/issues"
	},
	"homepage": "https://github.com/sakorman/react-three-interaction#readme",
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0",
		"three": ">=0.150.0"
	},
	"dependencies": {
		"@react-three/drei": "^9.92.7",
		"@react-three/fiber": "^8.15.12",
		"clsx": "^2.0.0",
		"styled-components": "^6.1.6",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@babel/core": "^7.23.6",
		"@babel/preset-env": "^7.23.6",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.30.0",
		"@types/react": "^18.2.45",
		"@types/react-dom": "^18.2.18",
		"@types/styled-components": "^5.1.34",
		"@types/three": "^0.158.3",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"babel-loader": "^9.1.3",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"html-webpack-plugin": "^5.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"three": "^0.158.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	},
	"engines": {
		"node": ">=16.0.0"
	}
}